<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomSeekbarStyle" >
        <item name="android:maxHeight">12dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@color/face</item>
        <item name="android:progressDrawable">@drawable/seekbar_v</item>
        <item name="android:minHeight">12dp</item>
        <item name="android:thumb">@drawable/seekbar_c</item>
    </style>

    <style name="CustomSeekbarStyle2" >
        <item name="android:maxHeight">12dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@color/jindu_ok</item>
        <item name="android:progressDrawable">@drawable/seekbar_jindutiao</item>
        <item name="android:minHeight">12dp</item>
        <item name="android:thumb">@drawable/seekbar_c</item>
    </style>

    <style name="eidt_bubble_dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
    </style>

    <style name="create_huanzhe_dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>

    </style>

    <style name="dialogstyle">
        <item name="@android:windowEnterAnimation">@anim/editdialog_enter</item>
        <item name="@android:windowExitAnimation">@anim/editdialog_exit</item>
    </style>

    <style name="floating_button" parent="TextAppearance.AppCompat">
        <item name="@android:textColor">#66343041</item>
    </style>
    <color name="colorAccent">#66343041</color>
</resources>
